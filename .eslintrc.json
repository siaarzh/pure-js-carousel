{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"off",
        "no-param-reassign":"warn"
    }
}
